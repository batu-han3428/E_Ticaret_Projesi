            @await Component.InvokeAsync("alisverisKredisiSecenekleri")
            <!-- /Alışveriş Kredisi Seçenekleri -->
            <!-- Bilgi -->
            @await Component.InvokeAsync("bilgi")
            <!-- /Bilgi -->
            <!-- Açıklama Metni-->
            @await Component.InvokeAsync("aciklamaMetni")
            <!-- /Açıklama Metni-->

        </div>
