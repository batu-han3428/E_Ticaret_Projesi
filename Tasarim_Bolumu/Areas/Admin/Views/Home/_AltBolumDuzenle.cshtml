            </div>
        </div>
    </div>
    <!-- /İçerik Başlık -->
    <div class="row">
        <div class="col-md-12">
            <hr>
            <div class="row">
                <div class="col-md-12">
                    <span class="text-muted" style="cursor:default;">Değişiklik yapmak istediğiniz bölüme tıklayınız..</span>
                </div>
            </div>
            <hr />

            <!-- Ödeme Seçenekleri -->
            @await Component.InvokeAsync("odemeSecenekleri")
            <!-- /Ödeme Seçenekleri -->
